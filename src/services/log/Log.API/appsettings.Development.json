{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{APP_INSIGHTS}"
  },
  "AzureStorage": {
    "ConnectionString": "{STORAGE_CONNECTION}"
  },
  "ContainerBlob": "logs",
  "TokenValidationParameters": {
    "ValidAudience": "https://localhost:44376"
  },
  "HealthChecks-UI": {
    "HealthChecks": [
      {
        "Name": "Log Tags Health",
        "Uri": "https://localhost:44322/health"
      }
    ],
    "Webhooks": [],
    "EvaluationTimeOnSeconds": 150,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}