{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{version}/reads",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "read.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/reads",
      "UpstreamHttpMethod": [ "Get" ],
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 3,
        "DurationOfBreak": 10000,
        "TimeoutValue": 5000
      },
      "FileCacheOptions": {
        "TtlSeconds": 15
      }
    },
    {
      "DownstreamPathTemplate": "/api/{version}/logs",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "log.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/logs",
      "UpstreamHttpMethod": [ "Get" ],
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 3,
        "DurationOfBreak": 10000,
        "TimeoutValue": 5000
      },
      "FileCacheOptions": {
        "TtlSeconds": 15
      }
    }
  ],
  "GlobalConfiguration": {}
}